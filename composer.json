{
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "name": "ryunosuke/functions",
  "description": "Utility functions for PHP",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ryunosuke\\Functions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\Test\\": "tests/Test/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*",
    "ryunosuke/phpunit-extension": "3.*",
    "psr/simple-cache": "*",
    "psr/log": "*"
  },
  "bin": [
    "bin/export-function",
    "bin/exclude-internal"
  ],
  "scripts": {
    "build": [
      "@export",
      "@stub",
      "@test"
    ],
    "stub": [
      "php bin/stub.php"
    ],
    "export": [
      "ryunosuke\\Functions\\Transporter::build"
    ],
    "test": [
      "php vendor/bin/phpunit --configuration=tests/phpunit.xml"
    ]
  }
}
